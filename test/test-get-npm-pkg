'use strict';

const chai = require('chai');
const get_npm_pkg = require('../lib/util/get-npm-pkg');

const expect = chai.expect;

// to run all tests start with test-
// npm test
// OR
// stage_env=test mocha --timeout 100000 --reporter spec test/test-get-npm-pkg

describe('Test check-latest-version', () => {

    it('test get_npm_pkg', async () => {
        const result  = await get_npm_pkg();
        expect(result.pkg.name).equals(result.npm_pkg.name)
    });

});